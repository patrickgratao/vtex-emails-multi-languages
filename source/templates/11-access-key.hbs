<table width="100%" border="0" cellpadding="0" cellspacing="0">
        <tr>
            <td align="center" valign="top">
                <table class="w-100 mw6-5 bg-white email__wrapper" width="100%" align="center" border="0" cellpadding="0"
                    cellspacing="0">

                    <!-- Header -->
                    <tr>
                        <td class="">
                          <!-- partial:partials/main/_header.hbs -->
                          <!-- partial -->
                        </td>
                    </tr>

                    <tr>
                      <td class="w-100 ph4-ns pt1 custom__text">
                        <p>
                            {{#if to}}
                            {{#if to.0.name}}
                            {{#compare to.0.name '!=' 'isAnonymous'}}
                            ((i18n.common.hi)), {{to.0.name}}.
                            {{/compare}}
                            {{/if}}
                            {{/if}}
                        </p>
                      </td>
                    </tr>

                    <tr>
                        <td class="w-100 ph4-ns pb1 custom__text">
                            <p class="mt4">((i18n.accesskey.Confirmation))</p>
                            <p>((i18n.accesskey.Return))</p>
                        </td>
                    </tr>

                    <tr>
                        <td class="w-100 ph4-ns pt1 pb1">
                            <div
                                style='background: #ffffff; border: 1px solid #ffffff; color: #000000;text-align:center;font-size:42px;padding:20px;'>
                                <strong class="mt4" style="font-size: 2rem; letter-spacing: 1px; color: #CA0538">{{aditionalData.accessKey}}</strong>
                            </div>
                        </td>
                    </tr>


                    <tr>
                        <td class="ph4-ns pv3 bt custom__text" style="border: none;">
                            <!-- partial:partials/messages/regards.hbs -->
                            <!-- partial -->
                        </td>
                    </tr>
                </table>
            </td>
        </tr>
     <tr>
      <td align="center" valign="top">
        <!-- partial:partials/main/_footer.hbs -->
        <!-- partial -->
      </td>
    </tr>
  </table>
</body>

</html>
